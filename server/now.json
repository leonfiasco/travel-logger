{
	"version": 2,
	"name": "leons-travel-log",
	"builds": [
		{
			"src": "./index.js",
			"use": "@now/node-server"
		}
	],
	"routes": [
		{
			"src": "/.*",
			"dest": "./index.js"
		}
	],
	"alias": ["leons-travel-log"],
	"env": {
		"NODE_ENV": "production",
		"PORT": "1337",
		"DATABASE_URL": "@travel-log-db",
		"CORS_ORIGIN": "",
		"API_KEY": "@travel-api-key"
	}
}
